#
# CAN frame properties that can be manifested on the bus.
#

@union

Error.1.0       error                        # CAN error (intentional or disturbance)
DataFD.1.0      data_fd                      # Bit rate switch flag active
DataClassic.1.0 data_classic                 # Bit rate switch flag not active
RTR.1.0         remote_transmission_request  # Bit rate switch flag not active

@assert _offset_.min == 32
@assert _offset_.max == 32 + 8 + 64 * 8
@assert _offset_ % 8 == {0}
